{
  "name": "design-language-website",
  "version": "0.0.0",
  "main": "src",
  "repository": {
    "url": "git@github.com:carbon-design-system/gatsby-theme-carbon.git",
    "type": "git"
  },
  "author": "Alison Joseph <alison.joseph@us.ibm.com>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "yarn clean:cache && gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean:cache": "rm -rf .cache",
    "ci-check": "yarn format:diff",
    "format": "prettier --write \"**/*.{scss,css,js,md}\"",
    "format:staged": "prettier --write",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,md}\""
  },
  "dependencies": {
    "@carbon/elements": "^10.2.0",
    "carbon-components": "^10.2.0",
    "carbon-components-react": "^7.2.0",
    "gatsby": "^2.7.3",
    "gatsby-theme-carbon": "^0.7.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
