{
  "name": "design-language-website",
  "description": "IBM Design Language website",
  "private": true,
  "version": "0.0.0",
  "main": "src",
  "repository": {
    "url": "git@github.com:carbon-design-system/design-language-website.git",
    "type": "git"
  },
  "author": "Alison Joseph <alison.joseph@us.ibm.com>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "gatsby develop",
    "dev:clean": "gatsby clean && gatsby develop",
    "build": "gatsby build",
    "build:prefix": "gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "ci-check": "yarn format:diff",
    "format": "prettier --write \"**/*.{scss,css,js,md}\"",
    "format:staged": "prettier --write",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,md}\""
  },
  "dependencies": {
    "@carbon/elements": "^10.2.0",
    "@carbon/icons-react": "^10.4.0-alpha.5390",
    "@ibm/plex": "^2.0.0",
    "carbon-components": "^10.2.0",
    "carbon-components-react": "^7.2.0",
    "gatsby": "^2.9.4",
    "gatsby-theme-carbon": "^1.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "prettier": "^1.17.1"
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
